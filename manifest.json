{
  "manifest_version": 3,
  "name": "Floating Button Extension",
  "version": "1.0",
  "description": "Adds a floating button to any webpage.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"],
  "update_url": "https://raw.githubusercontent.com/jbermudezAcitech/kiosk/main/update.xml"

}
